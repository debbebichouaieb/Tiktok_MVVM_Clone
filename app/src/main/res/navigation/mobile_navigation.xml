<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.devchou.tiktok_mvvm_clone.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.devchou.tiktok_mvvm_clone.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment android:id="@+id/navigation_add_video"
        android:name="com.devchou.tiktok_mvvm_clone.ui.add_video.AddViewFragment"
        tools:layout="@layout/fragment_add_view"/>

    <fragment android:id="@+id/navigation_inbox"
        android:name="com.devchou.tiktok_mvvm_clone.ui.inbox.InboxFragment"
        tools:layout="@layout/fragment_inbox"/>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.devchou.tiktok_mvvm_clone.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>